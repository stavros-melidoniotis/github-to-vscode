{
    "name": "Github to vscode",
    "description": "Quickly open github repos with vscode.dev or clone a repo directly to vscode.",
    "version": "1.0.0",
    "manifest_version": 3,
    "icons": {
        "48": "./images/icons/icon-48.png",
        "128": "./images/icons/icon-128.png"
    },
    "background": {
        "service_worker": "./background.js"
    },
    "action": {
        "default_icons": {
            "48": "./images/icons/icon-48.png",
            "128": "./images/icons/icon-128.png"
        }
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "scripting"
    ],
    "host_permissions": [
        "*://github.com/*"
    ]
}